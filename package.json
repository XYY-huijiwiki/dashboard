{
  "name": "xyy-huijiwiki-dashboard",
  "version": "0.2.16",
  "description": "A sleek and modern dashboard for XYY Huijiwiki.",
  "main": "./out/main/index.js",
  "author": "Karsten Zhou",
  "homepage": "https://github.com/XYY-huijiwiki/dashboard",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:web": "npm run typecheck && electron-vite build --mode web"
  },
  "dependencies": {
    "@coleqiu/vue-drag-select": "^2.0.6-beta.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@google/model-viewer": "^4.0.0",
    "@monaco-editor/loader": "^1.5.0",
    "@tailwindcss/vite": "^4.0.3",
    "@vueuse/core": "^13.1.0",
    "base-x": "^5.0.0",
    "dayjs": "^1.11.13",
    "electron-debug": "^4.1.0",
    "electron-dl-manager": "^4.0.0",
    "file64": "^1.0.4",
    "filesize": "^10.1.6",
    "jssha": "^3.3.1",
    "jszip": "^3.10.1",
    "knex": "^3.1.0",
    "ky": "^1.7.5",
    "lodash-es": "^4.17.21",
    "mime-types": "^2.1.35",
    "monaco-editor": "^0.52.2",
    "octokit": "^4.1.0",
    "p-min-delay": "^4.1.0",
    "pinia": "^3.0.1",
    "tailwindcss": "^4.0.3",
    "uuid": "^11.1.0",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify/vue": "^5.0.0",
    "@shikijs/monaco": "^3.2.1",
    "@types/node": "^22.15.3",
    "@vicons/fa": "^0.13.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-updater": "^6.3.9",
    "electron-vite": "^3.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^10.0.0",
    "jiti": "^2.4.2",
    "mustache": "^4.2.0",
    "naive-ui": "^2.41.0",
    "prettier": "^3.3.2",
    "seemly": "^0.3.10",
    "shiki": "^3.1.0",
    "types-mediawiki": "^1.10.1",
    "typescript": "^5.8.3",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.2.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vue": "^3.4.30",
    "vue-tsc": "^2.0.22",
    "zx": "^8.3.2"
  },
  "overrides": {
    "types-mediawiki": {
      "vue": "$vue"
    }
  }
}
